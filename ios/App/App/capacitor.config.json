{
	"appId": "co.houseninja.app",
	"appName": "House Ninja",
	"bundledWebRuntime": false,
	"loggingBehavior": "debug",
	"npmClient": "yarn",
	"webDir": "dist",
	"android": {
		"allowMixedContent": false
	},
	"ios": {
		"contentInset": "never",
		"scrollEnabled": "false"
	},
	"server": {
		"hostname": "localhost",
		"iosScheme": "co.houseninja.app",
		"androidScheme": "co.houseninja.app",
		"cleartext": false,
		"allowNavigation": [
			"auth.houseninja.co",
			"houseninja.co",
			"*.houseninja.co"
		]
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0
		},
		"Keyboard": {
			"resize": "body"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"LocalNotifications": {
			"smallIcon": "",
			"iconColor": "",
			"sound": ""
		}
	},
	"cordova": {}
}
