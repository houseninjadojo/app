    "build": "ember build --environment=production",
    "build:env": "ember build --environment=$NODE_ENV",
    "commit": "commit",
    "env:load": "dotenv-vault pull --dotenvMe $DOTENV_IT_TOKEN",
    "env:push:ci": "dotenv-vault push ci",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint 'app/styles/**/*.css'",
    "lint:css:fix": "stylelint 'app/styles/**/*.css' --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ci": "percy exec -- ember test",
    "upload:sourcemaps:datadog": "datadog-ci sourcemaps upload dist --service $SERVICE --release-version $VERSION --minified-path-prefix $DD_SOURCEMAP_PREFIX"
  },
